<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
	<%= render 'layouts/shim'  %>
</head>
<body>

<%= render 'layouts/header' %>
<%= render 'layouts/menu' %>

<div class="container">


  <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "alert alert-#{key}") %>
  <% end %>


  <%= yield %>



  <%= render 'layouts/footer' %>

  <h4><%= debug(params) if Rails.env.development? %></h4>
  <h4><%= debug(assigns) if Rails.env.development? %></h4>
  <h4><%= query_diet_widget if Rails.env.development? %></h4>

</div>


</body>
</html>

