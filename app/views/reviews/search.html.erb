<% provide(:title, 'Search results') %>
<div class="container">
  <div class="row">
    <div class="span12">
	<h3>Found <%= pluralize(@reviews.count, 'result') %></h3>
      <% @reviews.each do |review| %>

          <%= render 'shared/preview_review', object: review %>

      <% end %>

    </div>
  </div>
</div>